  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4C566A"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.section",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.brackets",
        "punctuation.parenthesis",
        "punctuation.brace"
      ],
      "settings": {
        "foreground": "#616E88"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Control Keywords (Conditionals, Loops)",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Class, Support, Type Names",
      "scope": [
        "entity.name.type",
        "support.class",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name",
        "support.type",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "name": "Function Names and Calls",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.method",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Variables, Properties, Parameters (general)",
      "scope": [
        "variable",
        "variable.other",
        "variable.declaration",
        "variable.parameter",
        "variable.other.property",
        "variable.other.object",
        "variable.object.property",        
        "keyword.other.unit",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Constants (language, support, character, escape)",
      "scope": [
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "Numeric Literals",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "name": "String Literals",
      "scope": [
        "string",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#8FBCBB"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#81A1C1"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control", "entity.other.attribute-name.id"],
      "settings": {
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#A3BE8C"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#BF616A"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#EBCB8B"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#88C0D0"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js",
        "meta.decorator entity.name.function",
        "meta.decorator punctuation.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D08770"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BF616A"
      }
    },
    {
      "name": "JSON Key - Level 0",
